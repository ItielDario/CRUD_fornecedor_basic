                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <!-- ##### Footer Area End ##### -->

    <!-- ##### All Javascript Script ##### -->
    <!-- jQuery-2.2.4 js -->
    <script src="/js/hotel/jquery/jquery-2.2.4.min.js"></script>
    <!-- Popper js -->
    <script src="/js/hotel/bootstrap/popper.min.js"></script>
    <!-- Bootstrap js -->
    <script src="/js/hotel/bootstrap/bootstrap.min.js"></script>
    <!-- All Plugins js -->
    <script src="/js/hotel/plugins/plugins.js"></script>
    <!-- Active js -->
    <script src="/js/hotel/active.js"></script>
    <!-- Active js -->
    <script src="/js/index.js"></script>
